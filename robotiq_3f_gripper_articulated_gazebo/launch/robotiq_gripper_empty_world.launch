<?xml version="1.0"?>
<launch>

  <!-- startup simulated world -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" default="worlds/empty.world"/>
    <arg name="paused" value="true"/>
  </include>

  <!-- upload gripper urdf to robot_description -->
  <include file="$(find robotiq_3f_gripper_visualization)/launch/robotiq_gripper_upload.launch">
  </include>

  <!-- spawn in gazebo -->
  <node name="spawn_gazebo_model" pkg="gazebo_ros" type="spawn_model" args="-urdf -param robot_description -model robot -z 0.1" respawn="false" output="screen" />


  <!-- load joint controller configuration from YAMLfile to parameter service -->
  <rosparam file="$(find robotiq_3f_gripper_articulated_gazebo)/cfg/robotiq_3f_gripper_articulated_gazebo_controller.yaml" command="load"/>

  <!-- load the controller -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen"
  args="s_model_palm_finger_1_joint_position_controller
  s_model_finger_1_joint_1_position_controller
  s_model_finger_1_joint_2_position_controller
  s_model_finger_1_joint_3_position_controller
  s_model_palm_finger_2_joint_position_controller
  s_model_finger_2_joint_1_position_controller
  s_model_finger_2_joint_2_position_controller
  s_model_finger_2_joint_3_position_controller
  s_model_finger_middle_joint_1_position_controller
  s_model_finger_middle_joint_2_position_controller
  s_model_finger_middle_joint_3_position_controller
  joint_state_controller"/>


  <!-- Start the publisher for the left hand (the only hand) -->
  <node pkg="robot_state_publisher" type="robot_state_publisher" name="robotiq_hands_l_hand_robot_state_publisher">
    <param name="publish_frequency" type="double" value="50.0" />
   <!--  <remap from="joint_states" to="/robotiq_hands/left_hand/joint_states" /> -->
  </node>

</launch>
